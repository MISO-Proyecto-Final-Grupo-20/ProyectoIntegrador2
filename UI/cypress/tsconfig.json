{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "es2015",
    "lib": ["es2015", "dom"],
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "../cypress.config.ts"
  ]
}